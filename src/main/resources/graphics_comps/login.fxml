<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="300.0" prefWidth="250.0" spacing="5.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphics_comps.LoginController">
   <children>
      <AnchorPane prefHeight="358.0" prefWidth="240.0" styleClass="fundo-login" stylesheets="@login&amp;cadastro.css">
         <children>
            <Label layoutX="30.0" layoutY="29.0" prefHeight="65.0" prefWidth="190.0" styleClass="logo-login" stylesheets="@login&amp;cadastro.css" text="SeuBanco.com" />
            <PasswordField fx:id="entradaSenha" layoutX="49.0" layoutY="137.0" promptText="Senha" styleClass="entrada-dados" stylesheets="@login&amp;cadastro.css" />
            <Button fx:id="botaoEntrar" layoutX="81.0" layoutY="215.0" onAction="#entrar" prefHeight="32.0" prefWidth="72.0" styleClass="botao-entrar" stylesheets="@login&amp;cadastro.css" text="Entrar" />
            <TextField fx:id="entradaCpf" layoutX="49.0" layoutY="111.0" promptText="CPF" styleClass="entrada-dados" stylesheets="@login&amp;cadastro.css" />
            <CheckBox fx:id="checkColaborador" layoutX="47.0" layoutY="186.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="156.0" styleClass="checkbox-colaborador" stylesheets="@login&amp;cadastro.css" text="Entrar como colaborador">
               <font>
                  <Font size="9.0" />
               </font>
            </CheckBox>
          <Button fx:id="botaoCadastro" layoutX="73.0" layoutY="247.0" mnemonicParsing="false" onAction="#cadastrar" prefHeight="30.0" prefWidth="87.0" styleClass="botao-cadastrar" stylesheets="@login&amp;cadastro.css" text="Cadastrar" underline="true" />
            <CheckBox fx:id="checkVisivel" layoutX="203.0" layoutY="141.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="17.0" styleClass="checkbox-colaborador" stylesheets="@login&amp;cadastro.css">
               <font>
                  <Font size="9.0" />
               </font>
            </CheckBox>
         </children>
      </AnchorPane>
   </children>
</VBox>
